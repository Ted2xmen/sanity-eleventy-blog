<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <link href="https://unpkg.com/prismjs@1.20.0/themes/prism-okaidia.css" rel="stylesheet">

    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
      {# <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}"> #}
    {% set css %}
      {% include "style.css" %}
    {% endset %}
    <style>
      {{ css | cssmin | safe }}
    </style>
  </head>


  <body>


<section id="nav">
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <div class="container-fluid">
    <a class="navbar-brand" href="#">Ted2xmen 11ty Blog</a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav">
      
         {%- for nav in collections.nav  -%}

        <li class="m-1 nav-item{% if nav.url == page.url %} nav-item-active{% endif %}">

        

        <button type="button" class="btn btn-outline-dark">

        <a href="{{ nav.url | url }}">
        {{ nav.data.navtitle }}

        </a>
        </button>

       
        </li>
    
        {%- endfor -%}

         
      <li class="m-1 nav-item">

        

        <button type="button" class="btn btn-outline-dark">

        <a href="/posts/">
        Blog

        </a>
        </button>

       
        </li>
      
       
      </ul>
    </div>
  </div>
</nav>
</section>



<section id="header">
  <div class="container">
     
    <header>
    <div class="row">
  
        <div class="col-md-9">
              <h1 class="home"><a href="{{ '/' | url }}">{{ metadata.title }}</a> <img alt="GitHub last commit" src="https://img.shields.io/github/last-commit/ted2xmen/sanity-eleventy-blog"></h1>


<div class="container">
  <div class="row row row-cols-1 row-cols-md-3 g-2">
    <div class="col">
      <div class="card hover">
        <div class="card-body">
          <span><img src="https://upload.wikimedia.org/wikipedia/commons/thumb/e/ec/Medium_logo_Monogram.svg/1200px-Medium_logo_Monogram.svg.png" alt="" style="width: 24px;"></span> <a href="https://ted2xmen.medium.com/list/react-c6e687c31344" target="_blank">Medium</a>
        </div>
      </div>
    </div>

    <div class="col">
      <div class="card hover">
        <div class="card-body">
          <span><img src="https://github.githubassets.com/images/modules/logos_page/GitHub-Mark.png" alt="" style="width: 24px;"></span> <a href="https://github.com/Ted2xmen/" target="_blank">Github</a>
        </div>
      </div>
    </div>
	
    <div class="col">
      <div class="card hover">
        <div class="card-body">
          <span><img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/1116997/codepen.png" alt="" style="width: 24px;"></span> <a href="https://codepen.io/ted2xmen" target="_blank">Codepen</a>
        </div>
      </div>
    </div>
  </div>
</div>

			  
        </div>
        
        <div class="col-md-3">
            <img src="https://pbs.twimg.com/profile_images/1392896862093459456/tcTntWXP_400x400.jpg" alt="me" width="250px" height="250px">
        </div>
    </div>
  </header>     
            
  </div>
  </section>


<section id="second-header">
<div class="container">


		



</div>
</section>




<section id="content">
<div class="container">



    <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>
      {{ content | safe }}
    </main>

</div>
</section>






<section id="footer">
<div class="container">
    <footer>
        <p><em>This is an <a href="https://www.11ty.io/">Eleventy</a> + <a href="https://www.sanity.io">Sanity.io</a> project made by Ted2xmen</em></p>
    </footer>
</div>
</section>

    <!-- Current page: {{ page.url | url }} -->

        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

  </body>
</html>
